FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      boundary;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters     1;

vertices
(
    (0 0 0)
    (2 0 0)
    (2 2 0)
    (0 2 0)
    (0 0 2)
    (2 0 2)
    (2 2 2)
    (0 2 2)
);

blocks
(
    hex (0 1 2 3 4 5 6 7) (2 2 2) simpleGrading (1 1 1);
);

edges
(
    arc 0 1 (1 0.5 0)
    arc 0 1 (1 0.5 2)
);

boundary
{
    movingWall
    {
        type    wall;
        faces
        (
            (3 7 6 2)
        );
    }

    fixedWalls
    {
        type    wall;
        faces
        (
            (0 4 7 3)
            (0 4 5 1)
            (1 2 6 5)
        );
    }

    frontAndBack
    {
        type    empty;
    }
}




