FoamFile
{
    format          ascii;
    class           dictionary;
    location        "system";
    object          sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type                sets;

libs                ("libsampling.so");

setFormat           raw;

interpolationScheme cellPointFace;

fields
(
    U
    p
);

sets
(
    line1
    {
        //type        lineCell;
        type        lineUniform;
        nPoints     11;
        axis        x;
        //start       (-0.5 0.5 0.5);
        //end         (1.5 0.5 0.5);        
        start       (0 0.5 0.5);
        end         (1 0.5 0.5);        
    }

    line2
    {
        //type        lineCell;
        type        lineUniform;
        nPoints     11;
        axis        y;
        //start       (0.5 -0.5 0.5);
        //end         (0.5 1.5 0.5);
        start       (0.5 0 0.5);
        end         (0.5 1 0.5);
    }
);

